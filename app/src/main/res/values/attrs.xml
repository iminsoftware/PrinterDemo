<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TitleLayout">
        <attr name="title" format="string"/>
        <attr name="leftTitle" format="string"/>
        <attr name="leftImage" format="integer"/>
        <attr name="back" format="boolean"/>
        <attr name="rightImage" format="integer"/>
        <attr name="titleTextColor" format="color"/>
        <attr name="titleTextSize" format="dimension"/>
        <attr name="leftVisible" format="boolean"/>
        <attr name="leftTitleVisible" format="boolean"/>
        <attr name="rightVisible" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="TagFlowLayout">
        <attr name="max_select" format="integer"></attr>
        <attr name="tag_gravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>
</resources>